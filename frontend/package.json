{
  "name": "gitactionboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start-mock-api-server": "mockoon-cli start -D --data mock-data/data.json",
    "serve-with-mock-data": "concurrently --kill-others \"npm run start-mock-api-server\" \"npm run serve\" ",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix --max-warnings 0",
    "lint:fix": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.24.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@mockoon/cli": "^2.1.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "concurrently": "^7.3.0",
    "eslint-plugin-vue": "^9.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-strongly-recommended",
      "plugin:vue/vue3-recommended",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
