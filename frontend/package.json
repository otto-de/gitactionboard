{
  "name": "gitactionboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite dev",
    "start-mock-api-server": "mockoon-cli start --data mock-data/data.json",
    "serve-with-mock-data": "concurrently --kill-others \"npm run start-mock-api-server\" \"npm run serve\" ",
    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint --no-fix --max-warnings 0 . && stylelint '**/*.vue'",
    "lint:fix": "eslint --fix . && stylelint '**/*.vue' --fix"
  },
  "dependencies": {
    "chart.js": "^4.4.8",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-autocolors": "^0.3.1",
    "date-fns": "^4.1.0",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@mdi/js": "^7.4.47",
    "@mockoon/cli": "^9.1.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "flush-promises": "^1.0.2",
    "globals": "^15.15.0",
    "jest-serializer-vue-tjw": "^3.20.0",
    "jsdom": "^26.0.0",
    "neostandard": "^0.12.1",
    "postcss-html": "^1.8.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "vite": "^6.1.1",
    "vite-plugin-vuetify": "^2.1.0",
    "vitest": "^3.0.5",
    "vitest-canvas-mock": "^0.3.3"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-html/vue"
    ],
    "rules": {
      "no-empty-source": null
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
