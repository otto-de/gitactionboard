{
  "name": "gitactionboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite dev",
    "start-mock-api-server": "mockoon-cli start --data mock-data/data.json",
    "serve-with-mock-data": "concurrently --kill-others \"npm run start-mock-api-server\" \"npm run serve\" ",
    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint --no-fix --max-warnings 0 . && stylelint '**/*.vue'",
    "lint:fix": "eslint --fix . && stylelint '**/*.vue' --fix"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-autocolors": "^0.3.1",
    "date-fns": "^4.1.0",
    "vue": "^3.5.27",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^5.0.0",
    "vuetify": "^3.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@mdi/js": "^7.4.47",
    "@mockoon/cli": "^9.5.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.33.0",
    "flush-promises": "^1.0.2",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "neostandard": "^0.12.2",
    "postcss-html": "^1.8.1",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "vite": "^7.3.1",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^4.0.15",
    "vitest-canvas-mock": "^1.1.3",
    "vue3-snapshot-serializer": "^2.13.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-html/vue"
    ],
    "rules": {
      "no-empty-source": null
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
